<?xml version="1.0"?>
<extension key="io.compuco.custommosaico" type="module">
  <file>custommosaico</file>
  <name>Mosaico customizations</name>
  <description>Customizing Mosaico-CiviCRM integration behavior</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compucorp</author>
    <email>info@compucorp.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-10-18</releaseDate>
  <version>1.0.0-alpha.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.35</ver>
  </compatibility>
  <comments>
    Supported CiviCRM versions  :
    - 5.35.2
  </comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Custommosaico</namespace>
  </civix>
</extension>
